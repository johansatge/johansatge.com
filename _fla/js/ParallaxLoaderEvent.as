package js{		import flash.events.Event;		public class ParallaxLoaderEvent extends Event	{				private var loadedPercentage:Number;				/**		 * Inits the event		 * @param	String	the event ID		 * @param	Number	the loaded percentage		 */		public function ParallaxLoaderEvent(eventIdentifier:String , percentage:Number)		{			this.loadedPercentage = percentage;			super(eventIdentifier);		}				/**		 * Gets the loaded percentage		 * @return Number		 */		public function getLoadedPercentage():Number		{			return this.loadedPercentage;		}	}}